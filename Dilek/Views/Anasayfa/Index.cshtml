@model Domain.Domains.Images
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Anasayfa";

}

<div class="col-lg-12 text-center">
    <div class="col-lg-12 text-center">
        <img src="@Url.Content(@Model.FilePath)" class="img-full img-responsive" alt="Tarayıcınız Fotoğrafı Desteklemememektedir.">
        <h2>
            @Model.Code
            <br>
        </h2>
        <p>@Model.Text</p>
        <hr>
    </div>
</div>